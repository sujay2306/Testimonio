pragma solidity >= 0.8.9 <= 0.8.9;

contract ForensicEvidenceContract {
    string public evidences;

    function setEvidenceDetails(string memory e) public {
        evidences = e;	
    }

    function getData() public view returns (string memory) {
        return evidences;
    }

    constructor() public {
        evidences = "empty";
    }
}